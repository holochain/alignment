{
  "name": "app",
  "description": "Webcomponent lit-element following open-wc recommendations",
  "license": "MIT",
  "author": "lit-element",
  "version": "0.0.0",
  "scripts": {
    "start": "vite --port $UI_PORT --clearScreen false",
    "build": "vite build",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "package": "rm -f dist.zip && npm run build && cd dist && bestzip ../dist.zip *"
  },
  "dependencies": {
    "@scoped-elements/material-web": "0.0.19",
    "@open-wc/scoped-elements": "^2.1.4",
    "@shoelace-style/shoelace": "^2.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@holochain-open-dev/elements": "^0.7.0",
    "@holochain-open-dev/profiles": "^0.16.0",
    "@holochain-open-dev/file-storage": "^0.6.1",
    "@holochain-open-dev/stores": "^0.6.0",
    "@holochain-open-dev/utils": "^0.15.0",
    "@holochain/client": "^0.16",
    "@lightningrodlabs/we-applet": "0.9.0",
    "@lit-labs/context": "^0.2.0",
    "@lit/localize": "^0.11.4",
    "lit": "^2.7.0",
    "@ts-stack/markdown": "^1.4.0",
    "@types/sanitize-html": "=2.6.2",
    "wc-mermaid": "github:guillemcordoba/wc-mermaid#patch-1",
    "javascript-time-ago": "2.3.7",
