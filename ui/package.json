    "sanitize-filename": "1.6.3"
  },
  "devDependencies": {
    "@lit/localize-tools": "^0.6.3",
    "@open-wc/eslint-config": "^2.0.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "14.11.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "bestzip": "^2.2.1",
    "concurrently": "^5.1.0",
    "deepmerge": "^3.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.0",
    "vite": "^4.3.4",
